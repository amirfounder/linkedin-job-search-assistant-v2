{
    "scrapers": [
        {
            "name": "top_100_tech_companies",
            "url": "https://www.thomsonreuters.com/en/products-services/technology/top-100.html",
            "query": "Array.from(document.querySelector('tbody').children).map(x => x.children[0].innerText).slice(1)"
        },
        {
            "name": "linkedin_search_results",
            "url": "https://www.linkedin.com/search/results/people/?keywords={0}&origin=CLUSTER_EXPANSION&page={1}",
            "query": "Array.from(document.querySelectorAll('li.reusable-search__result-container')).map(e => e.querySelectorAll('a')[1]).map(e => ({link: e.href,name: e.innerText.split('\\n')[0]}))"
        }
    ]
}